{
  "name": "react-backend",
  "version": "0.0.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "concurrently -n \"express,react\" -c \"yellow,blue\" \"nodemon app.js\" \"cd frontend && npm run start\"",
    "install": "cd frontend && npm install",
    "start:prod": "node app.js",
    "heroku-postbuild": "cd frontend && npm install && npm run build",
    "jest": "jest"
  },
  "engines": {
    "npm": "6.x",
    "node": "13.x"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.17.3",
    "gfycat-style-urls": "^1.0.3",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^5.9.9",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.3",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.1.5",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
